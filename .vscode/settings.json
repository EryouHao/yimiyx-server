{
  "files.exclude": {
    "USE_GITIGNORE": true,
    "**/*.map": true,
    "**/*.js": {
      "when": "$(basename).ts"
    }
  },
  "path-intellisense.mappings": {
    "assets": "${workspaceRoot}/app/web/assets",
    "components": "${workspaceRoot}/app/web/components",
    "page": "${workspaceRoot}/app/web/page",
    "styles": "${workspaceRoot}/app/web/styles",
    "store": "${workspaceRoot}/app/web/store",
    "router": "${workspaceRoot}/app/web/router"
  },
  "typescript.tsdk": "node_modules/typescript/lib"
}