{
  "name": "veggie-admin",
  "version": "1.0.6",
  "description": "基于 easywebpack-vue 和 egg-view-vue-ssr 插件的 egg + vue + typescript 服务端渲染同构工程",
  "scripts": {
    "compress": "tar -zcvf veggie-admin.tar.gz --exclude=node_modules --exclude=ormconfig.json --exclude=**/*.ts --exclude=*.ts --exclude=app/web *",
    "compress1": "tar -zcvf veggie-admin.tar.gz --exclude=**/*.ts --exclude=app/web app config libs public run index.js",
    "tsc": "tsc -p tsconfig.json",
    "tsc:w": "tsc -p tsconfig.json -w",
    "clean": "rm -rf app/**.js && rm -rf config/**.js && rm -rf index.js",
    "build": "cross-env easywebpack build prod",
    "build:dev": "cross-env easywebpack build dev",
    "build:test": "cross-env easywebpack build test",
    "dev": "node --max_old_space_size=4096 index.js",
    "start": "egg-scripts start --daemon --title=egg-server-veggie",
    "stop": "egg-scripts stop --title=egg-server-veggie",
    "start:win": "cross-env NODE_ENV=production EGG_SERVER_ENV=prod node index.js",
    "start:test": "cross-env EGG_SERVER_ENV=test node index.js",
    "kill": "pkill --signal SIGINT egg-vue-webpack-boilerplate",
    "kill-mac": "kill -9 $(lsof -i:7001 |grep 'node'|awk '{print $2}')",
    "kill-win": "taskkill -PID $(netstat -ano | findstr 7001|awk '{print $5}') -F",
    "restart": "npm run stop && npm run start",
    "tslint": "tslint .",
    "tslint:fix": "tslint --fix .",
    "eslint": "eslint .",
    "eslint:fix": "eslint --fix ."
  },
  "dependencies": {
    "axios": "^0.18.0",
    "countup": "^1.8.2",
    "egg": "^2.8.1",
    "egg-cors": "^2.0.0",
    "egg-logger": "^1.6.2",
    "egg-scripts": "^2.6.0",
    "egg-validate": "^1.0.0",
    "egg-view-vue-ssr": "^3.0.8",
    "extend": "~3.0.1",
    "iview": "^2.14.0-rc.2",
    "iview-area": "^1.5.17",
    "js-cookie": "^2.2.0",
    "jsbarcode": "^3.9.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.1",
    "mysql": "^2.15.0",
    "reflect-metadata": "^0.1.12",
    "server-side-render-resource": "^1.0.3",
    "tinymce": "^4.7.13",
    "typeorm": "^0.2.6",
    "vue": "^2.5.16",
    "vue-hot-reload-api": "^2.3.0",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.16",
    "vue-virtual-scroller": "^0.11.7",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "autod-egg": "^1.1.0",
    "autoprefixer": "^8.5.0",
    "babel-core": "^6.26.3",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-jsx-v-model": "^2.0.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
