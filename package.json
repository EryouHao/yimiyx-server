{
  "name": "veggie-admin",
  "version": "1.0.2",
  "description": "基于 easywebpack-vue 和 egg-view-vue-ssr 插件的 egg + vue + typescript 服务端渲染同构工程",
  "scripts": {
    "tsc": "tsc -p tsconfig.json",
    "tsc:w": "tsc -p tsconfig.json -w",
    "clean": "rm -rf app/**.js && rm -rf config/**.js && rm -rf index.js",
    "build": "cross-env easywebpack build prod",
    "build:dev": "cross-env easywebpack build dev",
    "build:test": "cross-env easywebpack build test",
    "build:prod": "cross-env easywebpack build prod",
    "dev": "npm run tsc:w & node index.js",
    "start:dev": "cross-env NODE_ENV=development EGG_SERVER_ENV=local node index.js",
    "start": "cross-env NODE_ENV=production EGG_SERVER_ENV=prod node index.js",
    "start:test": "cross-env EGG_SERVER_ENV=test node index.js",
    "kill": "pkill --signal SIGINT egg-vue-webpack-boilerplate",
    "kill-mac": "kill -9 $(lsof -i:7001 |grep 'node'|awk '{print $2}')",
    "kill-win": "taskkill -PID $(netstat -ano | findstr 7001|awk '{print $5}') -F",
    "restart": "npm run kill-port && npm run start",
    "lint": "tslint .",
    "lint:fix": "tslint --fix ."
  },
  "dependencies": {
    "axios": "^0.17.1",
    "countup": "^1.8.2",
    "egg": "^2.1.0",
    "egg-cors": "^2.0.0",
    "egg-logger": "^1.5.0",
    "egg-validate": "^1.0.0",
    "egg-view-vue-ssr": "^3.0.2",
    "extend": "~3.0.0",
    "iview": "^2.9.2",
    "iview-area": "^1.5.17",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "mysql": "^2.15.0",
    "reflect-metadata": "^0.1.12",
    "server-side-render-resource": "^1.0.0",
    "tinymce": "^4.7.7",
    "typeorm": "^0.1.12",
    "vue": "^2.5.13",
    "vue-hot-reload-api": "^2.1.0",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.13",
    "vue-virtual-scroller": "^0.11.4",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "autod-egg": "^1.0.0",
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.26.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.6.0",
    "cross-env": "^5.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "directory-named-webpack-plugin": "^2.2.3",
    "easywebpack-cli": "^3.5.2",
    "easywebpack-vue": "^3.6.0",
    "egg-logview": "^1.0.0",
    "egg-webpack": "^3.2.8",
    "egg-webpack-vue": "^2.0.0",
    "eslint": "^4.3.0",
    "eslint-config-egg": "^5.0.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-vue": "^2.0.1",
    "imagemin-webpack-plugin": "^1.5.2",
    "ip": "^1.1.5",
    "postcss-loader": "^2.0.6",
    "progress-bar-webpack-plugin": "^1.10.0",
    "pug": "^2.0.0-rc.4",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "ts-loader": "^3.2.0",
    "tslint": "^5.9.1",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.6.2",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "vconsole": "^3.0.0",
    "vue-template-compiler": "^2.4.4",
    "webpack-manifest-resource-plugin": "^2.0.2"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "ci": {
    "version": "6, 7, 8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/songyazhao/veggie-admin.git"
  },
  "tnpm": {
    "mode": "yarn"
  },
  "author": "syazhao@foxmail.com",
  "license": "MIT",
  "homepage": "https://github.com/songyazhao/veggie-admin",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
